<form action="{{ path('admin.saison.delete', {id: saison.id}) }}" method="POST" onsubmit="return confirm('Êtes-vous sûr de vouloir supprimer cette saison')">
	<input type="hidden" name="token" value="{{ csrf_token('delete' ~ saison.id) }}">
	<button type="submit" class="btn btn-danger">Supprimer</button>
</form>